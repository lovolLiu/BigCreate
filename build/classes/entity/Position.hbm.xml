<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="Position" table="position">
		<cache usage="read-write" /> <!-- 缓存 -->
		<id name="id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="positionName"/>
		<property name="department"/>
		<property name="salaryMin"/>
		<property name="salaryMax"/>
		<property name="workAddress"/>
		<property name="positionAdvantage"/>
		<property name="positionAddress"/>
		<property name="jobDescription"/>
		<property name="forwardEmail"/>
		<property name="createDate"/>
	</class>
</hibernate-mapping>
